{
    "name": "bic-engine/foundation",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-iconv": "*",
        "ffi/env": "^1.0",
        "bic-engine/ui": "^1.0",
        "bic-engine/async": "^1.0",
        "symfony/yaml": "^5.4|^6.0",
        "symfony/config": "^5.4|^6.0",
        "sebastian/environment": "^5.1",
        "symfony/expression-language": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Foundation\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.28",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "nunomaduro/collision": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Foundation\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
